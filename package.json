{
  "name": "lazy-di",
  "version": "1.3.0",
  "description": "Lazy Dependency Injection",
  "exports": {
    ".": "./index.mjs",
    "./helper": "./helper.mjs",
    "./lib": "./lib.mjs",
    "./loader": "./loader.mjs"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node --test",
    "lint": "eslint --config .eslintrc.js *.mjs test/*.mjs",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jue89/node-lazy-di.git"
  },
  "keywords": [
    "di"
  ],
  "author": "Juergen Fitschen <me@jue.yt>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jue89/node-lazy-di/issues"
  },
  "homepage": "https://github.com/jue89/node-lazy-di#readme",
  "devDependencies": {
    "eslint": "^8.34.0",
    "pre-commit": "^1.2.2",
    "standard-version": "^9.5.0"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
